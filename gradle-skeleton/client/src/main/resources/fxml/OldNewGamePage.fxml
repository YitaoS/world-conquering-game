<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="400.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="edu.duke.ece651.team8.client.controller.OldNewGameController">
    <left>
        <Button fx:id="old" mnemonicParsing="false" onAction="#oldGame" text="Old Game" prefWidth="350" prefHeight="400" />
    </left>
    <right>
        <Button fx:id="new" mnemonicParsing="false" onAction="#newGame" text="New Game" prefWidth="350" prefHeight="400" />
    </right>
</BorderPane>
